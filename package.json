{
  "name": "univeral-hmr-ssr-react-redux-dokku",
  "version": "1.0.0",
  "description": "Univeral JS application utilizing Hot Module Reloading, Server Side Rendering, Code Splitting with React, Redux, React Router 4 and a heroku like deploy setup with Dokku.",
  "main": "index.js",
  "scripts": {
    "development": "NODE_PATH=./src NODE_ENV=development node ./src/server/server.babel.js",
    "production": "NODE_PATH=./src NODE_ENV=production node ./src/server/server.babel.js",
    "build": "npm run build:client && npm run build:server",
    "build:server": "NODE_ENV=production webpack --config ./webpack/server.babel.js",
    "build:client": "NODE_ENV=production webpack --config ./webpack/client.babel.js",
    "lint": "./node_modules/.bin/eslint ./src",
    "build:langs": "./node_modules/.bin/babel scripts/translate.js | node"
  },
  "keywords": [
    "react",
    "redux",
    "hot-module-reloading",
    "server-side-rendering",
    "hmr",
    "ssr",
    "code-splitting",
    "webpack"
  ],
  "author": "Alexander J Ray <alexjamesray@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.0",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.8.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-redux-saga": "^0.4.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "postcss-nested": "^2.0.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^2.6.1",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "classnames": "^2.2.5",
    "colors": "^1.1.2",
    "express": "^4.15.3",
    "history": "^4.6.3",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-intl": "^2.3.0",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.4",
    "redux": "^3.7.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-saga": "^0.15.3",
    "regenerator-runtime": "^0.10.5"
  }
}